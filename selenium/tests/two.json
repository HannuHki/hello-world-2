{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 2,
  "steps": [
    {
      "type": "get",
      "url": "https://hn.algolia.com/?query=Cook&sort=byPopularity&prefix=false&page=0&dateRange=all&type=story"
    },
    {
      "type": "verifyTextPresent",
      "text": "Google Pressure Cookers and Backpacks, Get a Visit from the Feds"
    },
    {
      "type": "verifyElementValue",
      "locator": {
        "type": "xpath",
        "value": "//header[@class='page-header']/div[2]/div/input"
      },
      "value": "Cook"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "xpath",
        "value": "//header[@class='page-header']/div[2]/div/input"
      },
      "text": "Big Ben"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "'Temperamental' Big Ben was ringing up to six seconds late"
      }
    },
    {
      "type": "goBack"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
}